{
    "name": "realpage/marker",
    "type": "library",
    "description": "Automate keeping track of upstream Dockerfile updates",
    "keywords": [
        "realpage",
        "marker"
    ],
    "homepage": "https://github.com/realpage/marker",
    "license": "MIT",
    "authors": [
        {
            "name": "John Laswell",
            "email": "john.laswell@realpage.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.0",
        "symfony/console": "^3.1",
        "symfony/process": "^3.1",
        "illuminate/support": "^5.2",
        "monolog/monolog": "^1.19",
        "illuminate/config": "^5.2",
        "illuminate/filesystem": "^5.2",
        "guzzlehttp/guzzle": "^6.2",
        "knplabs/github-api": "^1.7",
        "vlucas/phpdotenv": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "RealPage\\Marker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Realpage\\Marker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --testsuite all"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
